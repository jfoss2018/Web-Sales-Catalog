{
  "name": "catalog",
  "version": "1.0.0",
  "description": "Web-Sales-Catalog",
  "main": "server/server.js",
  "engines": {
    "node": "10.9.0",
    "npm": "6.2.0"
  },
  "scripts": {
    "start": "set NODE_ENV=production&& node server/app.js",
    "server": "set NODE_ENV=development&& nodemon server/server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "set NODE_ENV=test&& mocha",
    "heroku-postbuild": "cd client&& npm install&& npm run build"
  },
  "author": "James Foss",
  "repository": {
    "type": "git",
    "url": "https://github.com/jfoss2018/catalog.git"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  }
}
